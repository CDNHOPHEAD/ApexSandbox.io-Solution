public List<String> fullNames(List<String> firstNames, List<String> lastNames) {
    //code here
    List<String> fullNames = new List<String>();

    for(String st: firstNames){
        fullNames.add(st);
    }

    for(String lastName : lastNames){
        String temp = ' '+lastName;
        fullNames.add(temp);
    }
    return fullNames;
}