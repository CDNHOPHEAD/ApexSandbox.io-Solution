public Id insertStudent(String name, String email) {
    // code here
    	apxio__Student__c newStudent = new apxio__Student__c();
        newStudent.Name = name;
        newStudent.apxio__Email__c = email;
        insert newStudent;

        Id newStudentId = [SELECT Id FROM apxio__Student__c WHERE Name =:newStudent.Name && apxio__Email__c =:newStudent.apxio__Email__c LIMIT 1];
        return newStudentId;

}