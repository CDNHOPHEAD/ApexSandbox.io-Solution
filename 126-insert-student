public Id insertStudent(String name, String email) {
    // code here
    	apxio__Student__c newStudent = new apxio__Student__c();
        newStudent.Name = name;
        newStudent.apxio__Email__c = email;
        insert newStudent;

        List<apxio__Student__c> newStudentList = [SELECT Id FROM apxio__Student__c WHERE Name =:newStudent.Name && apxio__Email__c =:newStudent.apxio__Email__c];

        for(apxio__Student__c student: newStudentList){
            return student.Id;
        }

}