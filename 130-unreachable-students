public List<apxio__Student__c> selectUnreachableStudents() {
    // code here
    List<apxio__Student__c> unreachableStudents = [SELECT apxio__Registration_Number__c, Name FROM apxio__Student__c
    WHERE apxio__Email__c = null AND
    apxio__Phone__c = null
    ORDER BY Name];

    return unreachableStudents;
}