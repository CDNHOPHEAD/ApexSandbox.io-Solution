public List<sObject> getListOfsObject(List<Account> accounts, List<Contact> contacts)
{
    // code here
    List<sObject> sObjectList = new List<sObject>();

        if(accounts.size()>=0){
            for(Account acct: accounts){
            sObjectList.add(acct);
        }
        }
        
        if(contacts.size()>=0){
            for(Contact con: contacts){
                    sObjectList.add(con);
                    }
        }
        return sObjectList;
    }
